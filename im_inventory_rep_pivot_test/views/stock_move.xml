<odoo>
    <data>
        <record model="ir.ui.view" id="im_sm_prod_tag">
            <field name="name">im_sm_prod_tag</field>
            <field name="inherit_id" ref="stock.view_move_tree"/>
            <field name="model">stock.move</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="after">
                    <field name="im_product_tag_p" optional="hide"/>
                    <field name="im_mrp_state" optional="hide"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>