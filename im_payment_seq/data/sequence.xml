            <field name="code">bank.payment.receive.sequence</field>
            <field name="prefix">BRV</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
<!--            <field name="company_id">1</field>-->
        </record>
    </data>

    <!--  ########################  -->
    <!--  CT  -->

    <data noupdate="1">
        <record id="sequence_im_account_payment_internal" model="ir.sequence">
            <field name="name">Internal Payment Receive Sequence</field>
            <field name="code">internal.payment.sequence</field>
            <field name="prefix">CT</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
