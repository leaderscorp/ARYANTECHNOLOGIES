<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--  Send  -->
    <!--  Cash  -->
    <data noupdate="1">
        <record id="sequence_im_account_payment_send_cash" model="ir.sequence">
            <field name="name">Cash Payment Send Sequence</field>
            <field name="code">cash.payment.send.sequence</field>
            <field name="prefix">CPV</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
<!--            <field name="company_id">1</field>-->
        </record>
    </data>
    <!--  Bank  -->
    <data noupdate="1">
        <record id="sequence_im_account_payment_send_bank" model="ir.sequence">
            <field name="name">Bank Payment Send Sequence</field>
            <field name="code">bank.payment.send.sequence</field>
            <field name="prefix">BPV</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
<!--            <field name="company_id">1</field>-->
        </record>
    </data>

    <!--  Receive  -->
    <!--  Cash  -->
    <data noupdate="1">
        <record id="sequence_im_account_payment_receive_cash" model="ir.sequence">
            <field name="name">Cash Payment Receive Sequence</field>
            <field name="code">cash.payment.receive.sequence</field>
            <field name="prefix">CRV</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
<!--            <field name="company_id">1</field>-->
        </record>
    </data>

