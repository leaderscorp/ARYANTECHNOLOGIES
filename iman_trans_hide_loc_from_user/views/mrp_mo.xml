<odoo>
    <record id="iman_mo_comp_col" model="ir.ui.view">
        <field name="name">iman_mo_comp_col</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_raw_ids']//tree//field[@name='quantity_done']" position="after">
                <field name="remaining_quantity" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>