<odoo>
    <data>
        <record model="ir.ui.view" id="im_view_wizard_report_form">
            <field name="name">Imaan Inventory Wizard Form</field>
            <field name="model">inventory.wizard.report</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="date_from"/>
                            <field name="date_to"/>
                        </group>
                        <group>
                        </group>
                    </group>
                    <footer>
                        <button name="get_report" string="Print Report" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
